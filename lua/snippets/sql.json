{
  "select-by-id": {
    "prefix": ["selid"],
    "body": [
      "select * from \"${1:table}\" where \"${2:id}\" = '${3:id}';"
    ],
    "description": "Select from table by id"
  },
  "cte": {
    "prefix": ["cte"],
    "body": [
      "with ${1:cte} as (",
      "\tselect * from \"${2:table}\"",
      ")"
    ],
    "description": "Cte"
  }
}
